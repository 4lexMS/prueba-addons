<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_conexion_view_form" model="ir.ui.view">
        <field name="name">res.conexion.view.form</field>
        <field name="model">res.conexion</field>
        <field name="inherit_id" ref="integralis_conexion.res_conexion_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='token_test']" position='after'>
                    <field name="sap_db_test" />
            </xpath>
            <xpath expr="//field[@name='token_prod']" position='after'>
                <field name="sap_db_prod" />
            </xpath>
        </field>
    </record>
</odoo>
